{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:7121"
      }
    }
  },
  "DISettings": {
    "MaxConcurrency": 5,
    "UpdateInterval": 10,
    "minDelayBetweenRequests": 200,

    "MSQLExpressServer": "localhost\\SQLEXPRESS",
    "MSQLExpressDatabase": "GeolocationDB",
    "MSQLExpressTrustServer": true,
    "MSQLExpressTrustServerCertificate":  true
  }
}